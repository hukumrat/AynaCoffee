@if (TempData["Message"] != null)
{
    @switch (TempData["Type"])
    {
        case "success":
            <script type="text/javascript">
                window.onload = function() {
                    iziToast.success({
                        title:'@TempData["Header"]',
                        message: '@TempData["Message"]'
                    });
                };
            </script>
            break;
        case "warning":
            <script type="text/javascript">
                window.onload = function() {
                    iziToast.warning({
                        title:'@TempData["Header"]',
                        message: '@TempData["Message"]'
                    });
                };
            </script>
            break;
        case "error":
            <script type="text/javascript">
                window.onload = function() {
                    iziToast.error({
                        title:'@TempData["Header"]',
                        message: '@TempData["Message"]'
                    });
                };
            </script>
            break;
        case "info":
            <script type="text/javascript">
                window.onload = function() {
                    iziToast.info({
                        title:'@TempData["Header"]',
                        message: '@TempData["Message"]'
                    });
                };
            </script>
            break;
    }
}